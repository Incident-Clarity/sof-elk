{
  "index_patterns": [
    "kape-*"
  ],
  "composed_of": [
    "settings-common",
    "mappings-corecommon",
    "mappings-string",
    "mappings-final"
  ],
  "template": {
    "settings": {
      "index": {
        "number_of_replicas": 0,
        "mapping": {
          "total_fields": {
            "limit": 5000
          }
        }
      }
    },
    "mappings": {
      "properties": {
        "type": {
          "type": "keyword"
        },
        "version": {
          "type": "integer"
        }
      }
    }
  },
  "_meta": {
    "provider": "SOF-ELK"
  }
}
