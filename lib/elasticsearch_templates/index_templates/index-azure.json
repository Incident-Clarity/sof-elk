{
  "index_patterns": [
    "azure-*"
  ],
  "composed_of": [
    "settings-common",
    "mappings-corecommon",
    "mappings-string",
    "mappings-ip_address",
    "mappings-geohash",
    "mappings-guid",
    "mappings-final"
  ],
  "template": {
    "settings": {
      "index": {
        "number_of_replicas": 0
      }
    },
    "mappings": {
      "properties": {
        "event_data": {
          "type": "object"
        },
        "type": {
          "type": "keyword"
        },
        "version": {
          "type": "integer"
        }
      }
    }
  },
  "_meta": {
    "provider": "SOF-ELK"
  }
}
