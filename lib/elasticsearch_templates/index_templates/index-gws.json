{
  "index_patterns": [
    "gws-*"
  ],
  "composed_of": [
    "settings-common",
    "mappings-corecommon",
    "mappings-string",
    "mappings-ip_address",
    "mappings-geohash",
    "mappings-final"
  ],
  "template": {
    "settings": {
      "index": {
        "number_of_replicas": 0
      }
    },
    "mappings": {
      "properties": {
        "google_workspace":{
          "properties": {
            "admin": {
              "properties": {
                "event_parameters": {
                  "properties": {
                    "OLD_VALUE": {
                      "type": "text"
                    },
                    "NEW_VALUE": {
                      "type": "text"
                    }
                  }
                }
              }
            }
          }
        },
        "type": {
          "type": "keyword"
        },
        "version": {
          "type": "integer"
        }
      }
    }
  },
  "_meta": {
    "provider": "SOF-ELK"
  }
}
