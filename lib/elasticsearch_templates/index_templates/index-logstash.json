{
  "index_patterns": [
    "logstash-*"
  ],
  "composed_of": [
    "settings-common",
    "mappings-corecommon",
    "mappings-string",
    "mappings-ip_address",
    "mappings-geohash",
    "mappings-port",
    "mappings-zeekuid",
    "mappings-final"
  ],
  "template": {
    "settings": {
      "index": {
        "number_of_replicas": 0
      }
    },
    "mappings": {
      "properties": {
        "event": {
          "properties": {
            "auth_result": {
              "type": "keyword"
            }
          }
        },
        "ttl": {
          "type": "integer"
        },
        "key_id": {
          "type": "keyword"
        }
      }
    }
  },
  "_meta": {
    "provider": "SOF-ELK"
  }
}
